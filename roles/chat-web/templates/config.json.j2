{
  "default_server_config": {
      "m.homeserver": {
          "base_url": "https://{{ domain['x'] }}",
          "server_name": "{{ top_level_domain }}"
      }
  },
  "disable_custom_urls": true,
  "disable_guests": true,
  "disable_login_language_selector": false,
  "disable_3pid_login": true,
  "brand": "{{ domain['chat'] }}",
  "integrations_ui_url": "https://scalar.vector.im.im/",
  "integrations_rest_url": "https://scalar.vector.im.im/api",
  "integrations_jitsi_widget_url": "https://scalar.vector.im.im/api/widgets/jitsi.html",
  "bug_report_endpoint_url": "https://riot.im/bugreports/submit",
  "defaultCountryCode": "US",
  "showLabsSettings": true,
  "features": {
      "feature_pinning": "labs",
      "feature_custom_status": "labs",
      "feature_custom_tags": "labs",
      "feature_state_counters": "labs"
  },
  "default_federate": false,
  "default_theme": "light",
  "roomDirectory": {
      "servers": [
          "{{ domain['x'] }}"
      ]
  },
  "welcomeUserId": "@riot-bot:{{ top_level_domain }}",
  "piwik": {
      "url": "https://piwik.riot.im.im/",
      "whitelistedHSUrls": ["https://matrix.org.org"],
      "whitelistedISUrls": ["https://vector.im.im", "https://matrix.org.org"],
      "siteId": 1
  },
  "enable_presence_by_hs_url": {
      "https://matrix.org": false
    }
  }